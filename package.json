{
  "name": "ngxcovalent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:prod": "npm run env -s && ng build --prod --aot && rm -f ./public/*.js && rm -f ./public/*.css && rm -f ./public/*.txt && rm -f ./public/*.png && rm -f ./public/*.woff && rm -f ./public/*.woff2 && rm -f ./public/*.ttf && rm -f ./public/*.svg && rm -f ./public/*.eot && rm -f ./public/*.json && rm -f ./public/*.min.js && rm -f ./public/*.min.css && rm -f ./public/*.ico && rm -f ./public/*.html && cp -r ./dist/*.* ./public/ && cp -r ./dist/svg/ ./public/ && cp -r ./dist/assets/ ./public/",
    "clear:public": "rm -f ./public/*.js && rm -f ./public/*.css && rm -f ./public/*.txt && rm -f ./public/*.png && rm -f ./public/*.woff && rm -f ./public/*.woff2 && rm -f ./public/*.ttf && rm -f ./public/*.svg && rm -f ./public/*.eot && rm -f ./public/*.json && rm -f ./public/*.min.js && rm -f ./public/*.min.css && rm -f ./public/*.ico && rm -f ./public/*.html",
    "start": "ng serve --proxy-config proxy.conf.js --open",
    "hmr": "ng serve --configuration hmr --proxy-config proxy.conf.js --open",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"angular/**/*.scss\" --syntax scss",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run write:env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./angular --output ./angular/translations/template.json --format=json --clean --sort",
    "docs": "hads ./docs -o",
    "compodoc": "compodoc -p tsconfig.json -d docs/generated -s -o",
    "write:env": "ngx-scripts env npm_package_version server_url api_url default_language",
    "generate": "ng generate"
  },
